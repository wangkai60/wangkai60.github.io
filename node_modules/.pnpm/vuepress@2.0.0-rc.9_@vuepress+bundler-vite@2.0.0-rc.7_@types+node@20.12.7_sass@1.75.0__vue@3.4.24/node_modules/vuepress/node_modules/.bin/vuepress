#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/wangkai/PycharmProjects/langchain-chinese-document/node_modules/.pnpm/vuepress@2.0.0-rc.9_@vuepress+bundler-vite@2.0.0-rc.7_@types+node@20.12.7_sass@1.75.0__vue@3.4.24/node_modules/vuepress/bin/node_modules:/Users/wangkai/PycharmProjects/langchain-chinese-document/node_modules/.pnpm/vuepress@2.0.0-rc.9_@vuepress+bundler-vite@2.0.0-rc.7_@types+node@20.12.7_sass@1.75.0__vue@3.4.24/node_modules/vuepress/node_modules:/Users/wangkai/PycharmProjects/langchain-chinese-document/node_modules/.pnpm/vuepress@2.0.0-rc.9_@vuepress+bundler-vite@2.0.0-rc.7_@types+node@20.12.7_sass@1.75.0__vue@3.4.24/node_modules:/Users/wangkai/PycharmProjects/langchain-chinese-document/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/wangkai/PycharmProjects/langchain-chinese-document/node_modules/.pnpm/vuepress@2.0.0-rc.9_@vuepress+bundler-vite@2.0.0-rc.7_@types+node@20.12.7_sass@1.75.0__vue@3.4.24/node_modules/vuepress/bin/node_modules:/Users/wangkai/PycharmProjects/langchain-chinese-document/node_modules/.pnpm/vuepress@2.0.0-rc.9_@vuepress+bundler-vite@2.0.0-rc.7_@types+node@20.12.7_sass@1.75.0__vue@3.4.24/node_modules/vuepress/node_modules:/Users/wangkai/PycharmProjects/langchain-chinese-document/node_modules/.pnpm/vuepress@2.0.0-rc.9_@vuepress+bundler-vite@2.0.0-rc.7_@types+node@20.12.7_sass@1.75.0__vue@3.4.24/node_modules:/Users/wangkai/PycharmProjects/langchain-chinese-document/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/vuepress.js" "$@"
else
  exec node  "$basedir/../../bin/vuepress.js" "$@"
fi
